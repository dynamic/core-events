{
  "name": "dynamic/core-events",
  "description": "Event Calendar for Dynamic Core",
  "keywords": [
    "silverstripe",
    "dynamic",
    "module",
    "core",
    "events"
  ],
  "license": "BSD-3-Clause",
  "type": "silverstripe-vendormodule",
  "require": {
    "dynamic/core": "^4.0",
    "silverstripe/recipe-cms": "^4.0",
    "silverstripe/timepickerfield": "dev-master",
    "johngrogg/ics-parser": "^v2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^3.0",
    "phploc/phploc": "^4.0",
    "pdepend/pdepend": "^2.5",
    "theseer/phpdox": "^0.11.0"
  },
  "authors": [
    {
      "name": "Dynamic",
      "homepage": "http://www.dynamicagency.com",
      "email": "dev@dynamicagency.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.packagist.com/dynamic/"
    }
  ],
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\Core\\": "src/",
      "Dynamic\\Core\\Test\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-name": "core-events",
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  }
}
